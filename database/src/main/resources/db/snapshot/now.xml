<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	logicalFilePath="/com/onbelay/obcore/now.xml"
>
	<!--
		Defines 'now' for various databases. (for inserts and
   updates
	-->
	<property
		dbms="oracle"
		name="now"
		value="sysdate"
	/>
	<property
		dbms="postgresql"
		name="now"
		value="current_date"
	/>
	<property
		dbms="mssql"
		name="now"
		value="getdate()"
	/>
</databaseChangeLog>